package exer;

public class SwitchTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch = 'C';
		
		switch (ch) {
		case 'A':
			System.out.println("입력문자는 A");
			
			break;
			
		case 'B':
			System.out.println("입력문자는 B");
			
			break;

		case 'C':
			System.out.println("입력문자는 C");
			
			break;


		default:
			break;
		}

	}

}
